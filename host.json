{
  "version": "2.0",
  "logging": {
      "console":{
          "isEnabled":true,
          "DisableColors":false
      },
      "applicationInsights": {
          "samplingSettings": {
              "isEnabled": true,
              "excludedTypes": "Request",
              "maxTelemetryItemsPerSecond": 30,
              "evaluationInterval": "02:00:00",
              "initialSamplingPercentage":"100.0",
              "samplingPercentageIncreaseTimeout":"00:00:01",
              "samplingPercentageDecreaseTimeout":"00:00:01",
              "minSamplingPercentage":"20.0",
              "maxSamplingPercentage":"100.0",
              "movingAverageRatio":"1.0",
              "includedTypes":null
          }
      },
      "fileLoggingMode": "always",
      "logLevel": {
          "Host.Triggers.Workflows":"Trace",
          "Host.Triggers":"Trace",
          "Host":"Trace",
          "default": "Trace"
      },
      "enableLiveMetrics": true,
      "enableDependencyTracking": true,
      "enablePerformanceCountersCollection": true,
      "httpAutoCollectionOptions": {
          "enableHttpTriggerExtendedInfoCollection": true,
          "enableW3CDistributedTracing": true,
          "enableResponseHeaderInjection": true
      },
      "snapshotConfiguration": {
          "agentEndpoint": null,
          "captureSnapshotMemoryWeight": 0.5,
          "failedRequestLimit": 3,
          "handleUntrackedExceptions": true,
          "isEnabled": true,
          "isEnabledInDeveloperMode": true,
          "isEnabledWhenProfiling": true,
          "isExceptionSnappointsEnabled": true,
          "isLowPrioritySnapshotUploader": true,
          "maximumCollectionPlanSize": 50,
          "maximumSnapshotsRequired": 3,
          "problemCounterResetInterval": "24:00:00",
          "provideAnonymousTelemetry": true,
          "reconnectInterval": "00:10:00",
          "shadowCopyFolder": null,
          "shareUploaderProcess": true,
          "snapshotInLowPriorityThread": true,
          "snapshotsPerDayLimit": 100,
          "snapshotsPerTenMinutesLimit": 3,
          "tempFolder": null,
          "thresholdForSnapshotting": 1,
          "uploaderProxy": null
      },  
        "managedDependency": {
        "enabled": true
    }
  },
  "healthMonitor": {
      "enabled": true,
      "healthCheckInterval": "00:00:10",
      "healthCheckWindow": "00:02:00",
      "healthCheckThreshold": 6,
      "counterThreshold": 0.80
  },
  "retry": {
    "strategy": "fixedDelay",
    "maxRetryCount": 5,
    "delayInterval": "00:00:05"
  },
  "singleton": {
    "lockPeriod": "00:00:15",
    "listenerLockPeriod": "00:01:00",
    "listenerLockRecoveryPollingInterval": "00:01:00",
    "lockAcquisitionTimeout": "00:01:00",
    "lockAcquisitionPollingInterval": "00:00:03"
  }
}